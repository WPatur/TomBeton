<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductServelet</servlet-name>
        <servlet-class>ProductServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KoszykServlet</servlet-name>
        <servlet-class>KoszykServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZamowienieServelet</servlet-name>
        <servlet-class>ZamowienieServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductServelet</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KoszykServlet</servlet-name>
        <url-pattern>/kosz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZamowienieServelet</servlet-name>
        <url-pattern>/zamowienie</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Baza</servlet-name>
        <servlet-class>Baza</servlet-class>
        <url-pattern>/Baza</url-pattern>
    </servlet>
    <servlet>
        <servlet-name>Log</servlet-name>
        <servlet-class>Log</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Baza</servlet-name>
        <url-pattern>/Baza/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Log</servlet-name>
        <url-pattern>/Log</url-pattern>
    </servlet-mapping>
</web-app>
